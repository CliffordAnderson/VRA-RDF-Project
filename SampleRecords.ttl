            vra:containedIn <http://vocab.getty.edu/tgn/1000003>,
                <http://vocab.getty.edu/tgn/1000070>,
                <http://vocab.getty.edu/tgn/7002883>,
                <http://vocab.getty.edu/tgn/7009061> ;
            vra:description "Courbevoie is a suburb of Paris in the Hauts-de-Seine department." ;
            vra:name "University of Notre Dame, Architecture Library (South Bend, Indiana, United States)" ] ;
    vra:placeOfRepository [ a vra:CivicStructure,
